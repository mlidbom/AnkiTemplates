<div id="container1">
   <div id="container2" class="{{Tags}} card{{Card}}">
      <!--######-->
      <div class="topSection">
         <div>{{Audio}}</div>
         <div class="image">{{Image}}</div>

         {{#__kanji}}<div class="kanji user">{{__kanji}}</div>{{/__kanji}}
         {{^__kanji}}<div class=kanji>{{Jlab-Kanji}}</div>{{/__kanji}}
      </div>

      <div class="bottomSection">
         {{#__comments}}<div class="my-comments">{{__comments}}</div>{{/__comments}}
         {{#RemarksBack}}<div>{{RemarksBack}}</div>{{/RemarksBack}}
      </div>

      <div class="vocab list">
         <div class="vocab entry">{{__vocab1}}</div>
         <div class="vocab entry">{{__vocab2}}</div>
         <div class="vocab entry">{{__vocab3}}</div>
      </div>

      <div class="bottomSection">
         <div>
            {{#__feedback-link}}<a href="{{__feedback-link}}">Sent feedback</a>{{/__feedback-link}}
            {{#__hinative}}<a href="{{__hinative}}">Hinative</a>{{/__hinative}}
            {{^__feedback-link}}<a href="mailto:joe@japanese-like-a-breeze.com?subject={{Version}}:{{Sequence}}">Mail feedback</a>{{/__feedback-link}}
         </div>

         {{#References}}<div>{{References}}</div>{{/References}}
         {{#Jlab-Remarks}}<div>Jlab-Remarks:{{Jlab-Remarks}}</div>{{/Jlab-Remarks}}
         {{#Other-Back}}<div>Other-Back:{{Other-Back}}</div>{{/Other-Back}}
      </div>
      <!--######-->
   </div>
</div>

<!-- Yomichan hack to get styles working right -->
<script>
   document.documentElement.setAttribute('data-glossary-layout-mode', 'compact');
   document.documentElement.setAttribute('data-theme', 'dark');
</script>